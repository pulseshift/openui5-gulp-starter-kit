{
  "name": "openui5-gulp-starter-kit",
  "version": "0.2.2",
  "description": "OpenUI5 template app build with gulp.",
  "keywords": ["pulseshift", "OpenUI5", "ui5"],
  "scripts": {
    "test": "yarn eslint ./src/openui5-todo-app --ext .js --cache",
    "start":
      "NODE_ENV=development babel-node node_modules/gulp/bin/gulp.js --silent",
    "start:verbose":
      "NODE_ENV=development babel-node node_modules/gulp/bin/gulp.js",
    "start:dist":
      "NODE_ENV=production babel-node node_modules/gulp/bin/gulp.js testDist --silent",
    "build":
      "NODE_ENV=production babel-node node_modules/gulp/bin/gulp.js build --silent",
    "build:verbose":
      "NODE_ENV=production babel-node node_modules/gulp/bin/gulp.js build",
    "build:disclaimer":
      "yarn licenses generate-disclaimer --production >> DISCLAIMER.md",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": ["prettier --write --no-semi --single-quote", "git add"]
  },
  "repository": {
    "url": "git+https://github.com/pulseshift/openui5-gulp-starter-kit.git",
    "type": "git"
  },
  "authors": [
    "Jascha Quintern <jascha.quintern@pulseshift.com>",
    "Michael Dell <michael.dell@pulseshift.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pulseshift/openui5-gulp-starter-kit/issues"
  },
  "homepage": "https://github.com/pulseshift/openui5-gulp-starter-kit#readme",
  "main": "src/index.hbs",
  "ui5": {
    "cacheBuster": true,
    "indexTitle": "UI5 Todo App",
    "src": "cdn",
    "srcLinks": {
      "githubRelease": {
        "url": "https://github.com/SAP/openui5/archive/{{version}}.zip",
        "version": "1.50.8",
        "isPrebuild": false,
        "isArchive": true
      },
      "githubMaster": {
        "url": "https://github.com/SAP/openui5/archive/master.zip",
        "version": "1.50.2-SNAPSHOT",
        "isPrebuild": false,
        "isArchive": true
      },
      "cdnDownload": {
        "url":
          "https://openui5.hana.ondemand.com/downloads/openui5-sdk-{{version}}.zip",
        "version": "1.50.8",
        "isPrebuild": true,
        "isArchive": true
      },
      "cdn": {
        "url":
          "https://openui5.hana.ondemand.com/{{version}}/resources/sap-ui-core.js",
        "version": "1.50.8",
        "isPrebuild": true,
        "isArchive": false
      },
      "local": {
        "url": "ui5/your-local-build/sap-ui-core.js",
        "version": "1.50.8",
        "isPrebuild": true,
        "isArchive": false
      }
    },
    "vendor": {
      "name": "app.libs",
      "path": "src/libs"
    },
    "theme": "sap_belize",
    "apps": [
      {
        "name": "app.todo",
        "path": "src/openui5-todo-app"
      }
    ],
    "libraries": [],
    "themes": [],
    "assets": []
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babelify": "^8.0.0",
    "browser-sync": "^2.23.6",
    "browserify": "^16.0.0",
    "commander": "^2.13.0",
    "del": "^3.0.0",
    "eslint": "^4.8.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-prettier": "^2.3.1",
    "gulp": "gulpjs/gulp#4.0",
    "gulp-babel": "^7.0.0",
    "gulp-clean-css": "^3.9.2",
    "gulp-cli": "^2.0.1",
    "gulp-handlebars-html": "^0.0.2",
    "gulp-htmlmin": "^4.0.0",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^4.1.0",
    "gulp-less": "^4.0.0",
    "gulp-main-npm-files": "^1.0.5",
    "gulp-plumber": "^1.2.0",
    "gulp-pretty-data": "^0.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-tap": "^1.0.1",
    "gulp-uglify": "^3.0.0",
    "gulp-ui5-preload": "^1.3.1",
    "gulp-util": "^3.0.8",
    "handlebars": "^4.0.11",
    "husky": "^0.14.3",
    "less": "^2.7.3",
    "less-plugin-autoprefix": "^1.5.1",
    "lint-staged": "^7.0.0",
    "ora": "^2.0.0",
    "prettier": "^1.10.2",
    "ui5-cache-buster": "^0.1.0",
    "ui5-lib-util": "^0.1.3",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  },
  "dependencies": {
    "moment": "^2.20.1"
  }
}
